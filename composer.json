{
  "name": "massimo-filippi/mail-module",
  "description": "ZF3 module for e-mail communication",
  "license": "MIT",
  "homepage": "https://github.com/massimo-filippi/mail-module",
  "authors": [
    {
      "name": "Matúš Petrofčík",
      "email": "matus@petrofcik.eu",
      "homepage": "https://www.petrofcik.eu",
      "role": "Developer"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^5.6 || ^7.0",
    "zendframework/zend-mail": "^2.8",
    "mailjet/mailjet-apiv3-php": "^1.2",
    "guzzlehttp/guzzle": "^6.3",
    "php-http/guzzle6-adapter": "^1.1",
    "sparkpost/sparkpost": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "MassimoFilippi\\MailModule\\": "src/"
    }
  },
  "extra": {
    "zf": {
      "module": "MassimoFilippi\\MailModule"
    },
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
